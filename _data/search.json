---
layout: null
---
[
  {% for post in site.posts %}
    {
      "title": "{{ post.title | escape }}",
      "excerpt": "{{ post.excerpt | strip_html | normalize_whitespace | escape }}",
      "tags": "{{ post.tags | join: ', ' }}",
      "url": "{{ post.url | relative_url }}"
    }
    {% unless forloop.last %},{% endunless %}
  {% endfor %}
]